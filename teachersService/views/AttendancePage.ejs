<body>


   <%- include("partials/navbarpage.ejs")  %>

    <div class="container justify-content-center align-self-center  min-vh-100">
        <div class="row  align-self-center  justify-content-center">
            <h2>USER - <label id="nameholder" ><%= nickname  %></label>(<label id='useridholder'><%= user_id%></label>)</h2>
            <h1>Proctoring</h1>
            <div class="col  align-self-center  justify-content-center" id="idstream">
                1 of 2
            </div>
            <div class="col   align-self-center  justify-content-center" id="result">
                <img src="" alt="to be taken" id="snappedImgSec">
            </div>
        </div>
        <div class="row">
            <div class="col" >
                <button type="button col-12 " class="btn btn-primary" id="takephoto"
                    style="">Take Image...</button>
            </div>
        </div>

        <div class="alert alert-success" role="alert" id='peoplefound' >
            People Found in The Previous Image -
          </div>

          <div class="alert alert-primary" role="alert" id='incrmessage'>
          </div>

